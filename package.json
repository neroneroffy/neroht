{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:build && npm run start:server",
    "clear:client": "rimraf dist/public",
    "clear:server": "rimraf dist/server-bundle",
    "start:build": "npm run clear:server && npm run clear:client && npm run build:client && npm run build:server",
    "start:server": "node \"./dist/server-bundle/root.server.js\"",
    "start:server:dev": "nodemon --watch server-bundle --exec node \"./dist/server-bundle/root.server.js\"",
    "build:server": "cross-env NODE_ENV=production npm run clear:server && webpack --config ./config/webpack.server.config.js --progress",
    "build:server:dev": "cross-env NODE_ENV=development npm run clear:server && webpack --config ./config/webpack.server.config.js --watch",
    "build:client": "cross-env NODE_ENV=production webpack --config ./config/webpack.client.config.js --progress",
    "server": "npm run clear:server && npm run build:server && npm run start:server",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.client.config.js",
    "build": "npm run clear:client && npm run build:client",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "execMap": {
      "npm": "npm"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "antd": "^3.19.2",
    "autoprefixer": "^9.5.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.11.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^5.5.0",
    "isomorphic-style-loader": "^4.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "loadable-components": "^2.2.3",
    "mini-css-extract-plugin": "^0.4.5",
    "nodemon": "^1.18.6",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.3.12",
    "react-loadable": "^5.5.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "compression": "^1.7.4",
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.16.4",
    "express-http-proxy": "^1.5.1",
    "moment": "^2.24.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-helmet": "^5.2.1",
    "react-infinite-scroller": "^1.2.4",
